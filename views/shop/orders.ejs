<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (orders.length <=0){ %> 
                <h1>Nothing there!</h1>
            <% }else{ %>
                <ul>
                    <% for (order of orders){ %> 
                    <li><%= order.id %> </li>
                    <ul>
                        <% for(item of order.Products){ %> 
                            <li><%= item.title %> (<%= item.orderItem.quantity %> ) </li>
                        <% } %> 
                    </ul>
                    <% } %> 
                </ul>
            <% } %>  
        </main>
        <%- include('../includes/end.ejs') %>